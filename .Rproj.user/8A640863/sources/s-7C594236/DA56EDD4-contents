cdcdata <- cdc
cdcdata$genhlth <- as.ordered(factor(cdcdata$genhlth,levels = c("poor", "fair", "good", "very good", "excellent")))
levels(cdcdata$genhlth)[levels(cdcdata$genhlth)=="very good"] <- "v.good"
# cdcdata$exerany <- as.ordered(factor(cdcdata$exerany))
# cdcdata$hlthplan <- as.ordered(factor(cdcdata$hlthplan))
# cdcdata$smoke100 <- as.ordered(factor(cdcdata$smoke100))
cdcdata$gender <- factor(cdcdata$gender)
cdcdata$age <- ggplot2::cut_interval(cdcdata$age,
                                     n = 4,
                                     ordered_result=T,
                                     right = F)
assoc_cdc <- calc_assoc(d = cdcdata)
plot_assoc_matrix(lassoc = assoc_cdc)


a <- read_xpt("~/Downloads/LLCP2021.XPT ")
b <- a[sample(1:nrow(a),5000),sample(1:ncol(a),10)]
assoc_b <- calc_assoc(d = b)
plot_assoc_matrix(lassoc = assoc_b)


